{% if traefik.middlewares.https_redirect -%}
http:
  routers:
    {% if traefik.middlewares.https_redirect -%}
    redirecttohttps:
      entryPoints:
        - "web"
      middlewares:
        - "httpsredirect"
      rule: "HostRegexp(`{host:.+}`)"
      service: "noop"
    {% endif %}
{% endif %}
